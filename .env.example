# Menentukan nama tampilan aplikasi contoh
APP_NAME=Laravel
# Lingkungan aplikasi default untuk contoh
APP_ENV=local
# Key enkripsi contoh (kosong agar diisi sendiri)
APP_KEY=
# Mode debug default agar error mudah dilihat saat pengembangan
APP_DEBUG=true
# URL dasar aplikasi contoh
APP_URL=http://localhost

# Channel log default yang menampung berbagai channel lain
LOG_CHANNEL=stack
# Channel khusus untuk mencatat deprecated warning
LOG_DEPRECATIONS_CHANNEL=null
# Level log minimum yang akan dicatat
LOG_LEVEL=debug

# Driver database default untuk contoh (MySQL)
DB_CONNECTION=mysql
# Host server database contoh
DB_HOST=127.0.0.1
# Port koneksi MySQL
DB_PORT=3306
# Nama database contoh
DB_DATABASE=ini_sempro
# Username MySQL contoh
DB_USERNAME=root
# Password MySQL contoh
DB_PASSWORD=

# Driver untuk broadcasting event
BROADCAST_DRIVER=log
# Driver untuk caching aplikasi
CACHE_DRIVER=file
# Disk filesystem default
FILESYSTEM_DISK=local
# Driver antrian tugas
QUEUE_CONNECTION=sync
# Driver penyimpanan sesi
SESSION_DRIVER=file
# Durasi sesi dalam menit
SESSION_LIFETIME=120

# Host memcached default
MEMCACHED_HOST=127.0.0.1

# Host Redis default
REDIS_HOST=127.0.0.1
# Password Redis default
REDIS_PASSWORD=null
# Port Redis default
REDIS_PORT=6379

# Driver pengiriman email default
MAIL_MAILER=smtp
# Host mail server default
MAIL_HOST=mailpit
# Port mail server default
MAIL_PORT=1025
# Username mail server default
MAIL_USERNAME=null
# Password mail server default
MAIL_PASSWORD=null
# Enkripsi mail server default
MAIL_ENCRYPTION=null
# Alamat pengirim default
MAIL_FROM_ADDRESS="hello@example.com"
# Nama pengirim default (menggunakan nama aplikasi)
MAIL_FROM_NAME="${APP_NAME}"

# ID akses AWS contoh
AWS_ACCESS_KEY_ID=
# Secret key AWS contoh
AWS_SECRET_ACCESS_KEY=
# Region AWS default contoh
AWS_DEFAULT_REGION=us-east-1
# Nama bucket AWS contoh
AWS_BUCKET=
# Menentukan penggunaan path style untuk S3
AWS_USE_PATH_STYLE_ENDPOINT=false

# ID aplikasi Pusher contoh
PUSHER_APP_ID=
# Key aplikasi Pusher contoh
PUSHER_APP_KEY=
# Secret aplikasi Pusher contoh
PUSHER_APP_SECRET=
# Host Pusher contoh
PUSHER_HOST=
# Port Pusher contoh
PUSHER_PORT=443
# Skema protokol Pusher contoh
PUSHER_SCHEME=https
# Cluster Pusher contoh
PUSHER_APP_CLUSTER=mt1

# Key Pusher untuk konfigurasi frontend Vite
VITE_PUSHER_APP_KEY="${PUSHER_APP_KEY}"
# Host Pusher untuk frontend Vite
VITE_PUSHER_HOST="${PUSHER_HOST}"
# Port Pusher untuk frontend Vite
VITE_PUSHER_PORT="${PUSHER_PORT}"
# Skema Pusher untuk frontend Vite
VITE_PUSHER_SCHEME="${PUSHER_SCHEME}"
# Cluster Pusher untuk frontend Vite
VITE_PUSHER_APP_CLUSTER="${PUSHER_APP_CLUSTER}"

# Konfigurasi dashboard contoh
DASHBOARD_MONTHLY_TARGET=5000000

# Nominal default iuran untuk form pembayaran pengguna
IURAN_SAMPAH_DEFAULT=50000
# Nominal default iuran ronda
IURAN_RONDA_DEFAULT=30000
# Nominal minimal iuran sampah
IURAN_SAMPAH_MIN=50000
# Nominal minimal iuran ronda
IURAN_RONDA_MIN=30000

